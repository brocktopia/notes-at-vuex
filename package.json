{
  "name": "notes-at",
  "description": "A note taking app that retrieves and stores location data for each note.",
  "version": "1.0.0",
  "author": "Brock Henderson <brocktopia@gmail.com>",
  "license": "MIT",
  "private": true,
  "browser": "./src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brocktopia/notes-at.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-dev": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "start": "nodemon api/server.js",
    "start-ssl": "nodemon api/server-ssl.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.8",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue2-google-maps": "^0.10.2",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-jest": "^1.0.0",
    "vue-loader": "^13.7.3",
    "vue-server-renderer": "^2.4.4",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.11.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue2-google-maps)"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
